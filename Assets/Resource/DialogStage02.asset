  dialogContent:
  - title: "2-1\u8CEA\u91CF\u5B88\u6046\u5B9A\u5F8B"
    question: "\u540C\u5B78\u62FF\u53D6\u9EDE\u706B\u5668\u89C0\u5BDF\u92FC\u7D72\u7D68\u3001\u6728\u5C51\u5728\u71C3\u71D2\u524D\u5F8C\u7684\u8B8A\u5316"
    warning: "\u4F7F\u7528\u9EDE\u706B\u69CD\u8ACB\u6CE8\u610F\u5B89\u5168!"
    voiceName: QT2_1
  - title: "2-2\u6C2F\u5316\u9223\u8207\u78B3\u9178\u9209\u53CD\u61C9"
    question: "\u540C\u5B78\u5C07\u6C2F\u5316\u9223\u6EB6\u6DB2\u8207\u78B3\u9178\u9209\u6EB6\u6DB2\u6DF7\u5408\u7684\u53CD\u61C9"
    warning: 
    voiceName: QT2_2
  - title: "2-3\u5C0F\u8607\u6253\u8207\u9E7D\u9178\u53CD\u61C9"
    question: "\u540C\u5B78\u64CD\u4F5C\u5C0F\u8607\u6253\u8207\u9E7D\u9178\u6DF7\u5408\u7684\u53CD\u61C9\uFF0C\u4E26\u85C9\u6B64\u63A2\u8A0E\u53CD\u61C9\u524D\u5F8C\u8CEA\u91CF\u7684\u8B8A\u5316"
    warning: "\u9E7D\u9178\u5177\u8150\u8755\u6027\uFF0C\u8ACB\u6CE8\u610F\u5B89\u5168\uFF01"
    voiceName: QT2_3
  - title: "2-1 \u53BB\u6C59\u9054\u4EBA"
    question: "\u5EDA\u623F\u88E1\u5145\u6EFF\u8457\u5404\u7A2E\u6CB9\u6C59\uFF0C\u5ABD\u5ABD\u8AAA\u53EA\u8981\u5229\u7528\u8D85\u5E02\u8CB7\u7684\u795E\u5947\u5C0F\u8607\u6253\u518D\u52A0\u4E0A\u6C34\uFF0C\u5C31\u53EF\u4EE5\u9806\u5229\u7684\u53BB\u9664\u6CB9\u6C61\u5566\uFF0C\u8B93\u6211\u5011\u8A66\u8A66\u770B\u5427\uFF01"
    warning: 
    voiceName: QE2_1
  - title: "2-2 \u53BB\u6C59\u9054\u4EBA"
    question: "\u5ABD\u5ABD\u4E4B\u524D\u6709\u8CB7\u4E86\u4E00\u5305\u653E\u5728\u9019\u88E1\uFF0C\u6211\u5011\u4E5F\u4F86\u8A66\u8A66\u770B\u53BB\u6C59\u529B\u5427!"
    warning: 
    voiceName: QE2_2
  - title: "2-3 \u5C0F\u5C0F\u51B6\u934A\u5E2B"
    question: "\u8001\u5E2B\u8AAA\u53EA\u8981\u7528\u78B3\u7C89\uFF0C\u5C31\u53EF\u4EE5\u628A\u9ED1\u8272\u7684\u6C27\u5316\u9285\u7C89\u8B8A\u56DE\u7D05\u8272\u7684\u9285\uFF0C\u6211\u5011\u8D95\u5FEB\u8A66\u770B\u770B\u5427\u3002"
    warning: 
    voiceName: QE2_3
