  dialogContent:
  - title: "1-1 \u8EDF\u9435\u7247\u8207\u5408\u91D1\u939A"
    question: "\u8ACB\u540C\u5B78\u62FF\u8D77\u5408\u91D1\u939A\uFF0C\u4E26\u4E14\u8A66\u8457\u6572\u64CA\u8EDF\u9435\u7247\u5427\u3002"
    warning: 
    voiceName: QT1_1_1
  - title: "1-2 \u8EDF\u9435\u7247\u8207\u9EDE\u706B\u5668"
    question: "\u540C\u5B78\u53EF\u4EE5\u62FF\u53D6\u5BE6\u9A57\u5BA4\u4E2D\u7684\u9EDE\u706B\u5668\u5C0D\u8EDF\u9435\u7247\u9032\u884C\u52A0\u71B1\u3002"
    warning: 
    voiceName: QT1_1_2
  - title: "1-3 \u8EDF\u9435\u7247\u8207\u9E7D\u9178"
    question: "\u540C\u5B78\u53EF\u4EE5\u62FF\u53D6\u8EDF\u9435\u7247\u518D\u653E\u5165\u9E7D\u9178\u4E2D\u3002"
    warning: 
    voiceName: QT1_1_3
  - title: "1-4 \u52A0\u71B1\u5C0F\u8607\u6253\u5BE6\u9A571"
    question: "\u540C\u5B78\u53EF\u4EE5\u5229\u7528\u5BE6\u9A57\u5668\u6750\u7D44\uFF0C\u85C9\u4EE5\u5B8C\u6210\u52A0\u71B1\u5C0F\u8607\u6253\u4E26\u6AA2\u6E2C\u5176\u7522\u7269\u7684\u5BE6\u9A57\u88DD\u7F6E\u3002"
    warning: 
    voiceName: QT1_1_4
  - title: "1-5 \u52A0\u71B1\u5C0F\u8607\u6253\u5BE6\u9A572"
    question: "\u540C\u5B78\u8ACB\u89C0\u5BDF\u6F84\u6E05\u77F3\u7070\u6C34\u5167\u767C\u751F\u7684\u8B8A\u5316\u3002"
    warning: 
    voiceName: QT1_1_5
  - title: "1-6 \u52A0\u71B1\u5C0F\u8607\u6253\u5BE6\u9A573"
    question: "\u8ACB\u540C\u5B78\u62FF\u8D77\u6C2F\u5316\u4E9E\u9237\u8A66\u7D19\u91DD\u5C0D\u6DB2\u6EF4\u9032\u884C\u6AA2\u6E2C\uFF0C\u4E26\u89C0\u5BDF\u8A66\u7D19\u7684\u984F\u8272\u8B8A\u5316\u3002"
    warning: 
    voiceName: QT1_1_6
  - title: "1-1 \u4E7E\u51B0\u5165\u6C34"
    question: "\u8ACB\u5C07\u5BE6\u9A57\u684C\u4E0A\u7684\u4E7E\u51B0\u4EE5\u9477\u5B50\u593E\u8D77\u4E26\u653E\u5165\u6C34\u4E2D\u3002"
    warning: 
    voiceName: QE1_1_1
  - title: "1-2 \u881F\u71ED\u71C3\u71D2"
    question: "\u540C\u5B78\u53EF\u4EE5\u89C0\u5BDF\u881F\u71ED\u7684\u8B8A\u5316\u60C5\u6CC1\u3002"
