  tipsContent:
  - chapter: T2_1_1
    content: "\u8ACB\u540C\u5B78\u6293\u53D6\u9EDE\u706B\u5668\uFF0C\u52A0\u71B1\u92FC\u7D72\u7D68\u53CA\u6728\u5C51\u3002"
  - chapter: T2_1_2
    content: "\u8ACB\u554F\u5316\u5B78\u53CD\u61C9\u7684\u524D\u5F8C\uFF0C\u8CEA\u91CF\u662F\u5982\u4F55\u8B8A\u5316\u5462\u3002"
  - chapter: T2_2_1
    content: "\u8ACB\u540C\u5B78\u5C07\u78B3\u9178\u9209\u6EB6\u6DB2\u5012\u5165\u5BF6\u7279\u74F6\u5167\uFF0C\u6C2F\u5316\u9223\u5012\u5165\u5C0F\u8A66\u7BA1\u4E2D\u3002"
  - chapter: T2_2_2
    content: "\u8ACB\u540C\u5B78\u4EE5\u9477\u5B50\u593E\u53D6\u88DD\u6709\u6C2F\u5316\u9223\u7684\u8A66\u7BA1\uFF0C\u50BE\u659C\u5C0F\u5FC3\u7684\u6ED1\u5165\u5BF6\u7279\u74F6\u3002"
  - chapter: T2_2_3
    content: "\u8ACB\u540C\u5B78\u9396\u4E0A\u5BF6\u7279\u74F6\u84CB\uFF0C\u4E26\u78BA\u8A8D\u6C2F\u5316\u9223\u6EB6\u6DB2\u8207\u78B3\u9178\u9209\u6EB6\u6DB2\u4E26\u7121\u63A5\u89F8\u3002"
  - chapter: T2_2_4
    content: "\u5404\u4F4D\u540C\u5B78\uFF0C\u63A5\u4E0B\u4F86\u8B93\u6211\u5011\u5229\u7528\u78C5\u79E4\u79E4\u91CF\u53CD\u61C9\u524D\u7684\u5BF6\u7279\u74F6\u7E3D\u8CEA\u91CF\u3002"
  - chapter: T2_2_5
    content: "\u5404\u4F4D\u540C\u5B78\uFF0C\u63A5\u4E0B\u4F86\u8B93\u6211\u5011\u50BE\u5012\u5BF6\u7279\u74F6\uFF0C\u8B93\u6EB6\u6DB2\u6DF7\u5408\uFF0C\u4F60\u770B\u5230\u4EC0\u9EBC\u8B8A\u5316\u5462\uFF1F"
  - chapter: T2_2_6
    content: "\u63A5\u4E0B\u4F86\u518D\u8B93\u6211\u5011\u5229\u7528\u78C5\u79E4\u79E4\u91CF\u53CD\u61C9\u5F8C\u7684\u5BF6\u7279\u74F6\u7E3D\u8CEA\u91CF\u3002"
  - chapter: T2_2_7
    content: "\u5404\u4F4D\u540C\u5B78\uFF0C\u63A5\u4E0B\u4F86\u8B93\u6211\u5011\u6253\u958B\u5BF6\u7279\u74F6\uFF0C\u6700\u5F8C\u518D\u5229\u7528\u78C5\u79E4\u79E4\u91CF\u958B\u74F6\u5F8C\u7684\u5BF6\u7279\u74F6(\u542B\u74F6\u84CB)\u7E3D\u8CEA\u91CF\u3002"
  - chapter: T2_2_8
    content: "\u5BE6\u9A57\u7D50\u679C\u53EF\u77E5\uFF0C\u5316\u5B78\u53CD\u61C9\u7684\u524D\u5F8C\u8CEA\u91CF\u662F\u5B88\u6046\u7684\u3002"
  - chapter: T2_3_1
    content: "\u8ACB\u540C\u5B78\u5C07\u5C0F\u8607\u6253\u6EB6\u6DB2\u5012\u5165\u5BF6\u7279\u74F6\u5167\uFF0C\u9E7D\u9178\u5012\u5165\u5C0F\u8A66\u7BA1\u4E2D\u3002"
  - chapter: T2_3_2
    content: "\u8ACB\u540C\u5B78\u4EE5\u9477\u5B50\u593E\u53D6\u88DD\u6709\u9E7D\u9178\u7684\u8A66\u7BA1\uFF0C\u50BE\u659C\u5C0F\u5FC3\u7684\u6ED1\u5165\u5BF6\u7279\u74F6\u3002 "
  - chapter: T2_3_3
    content: "\u8ACB\u540C\u5B78\u4EE5\u6C23\u7403\u5957\u4E0A\u5BF6\u7279\u74F6\u53E3\uFF0C\u4E26\u4EE5\u6A61\u76AE\u7B4B\u7D81\u7DCA\u5F8C\uFF0C\u78BA\u8A8D\u9E7D\u9178\u8207\u5C0F\u8607\u6253\u4E26\u7121\u63A5\u89F8
      \u3002 "
  - chapter: T2_3_4
    content: "\u5404\u4F4D\u540C\u5B78\uFF0C\u63A5\u4E0B\u4F86\u8B93\u6211\u5011\u5229\u7528\u78C5\u79E4\u79E4\u91CF\u53CD\u61C9\u524D\u7684\u5BF6\u7279\u74F6\u7E3D\u8CEA\u91CF\u3002"
  - chapter: T2_3_5
    content: "\u5404\u4F4D\u540C\u5B78\uFF0C\u63A5\u4E0B\u4F86\u8B93\u6211\u5011\u50BE\u5012\u5BF6\u7279\u74F6\uFF0C\u8B93\u6EB6\u6DB2\u6DF7\u5408\uFF0C\u4F60\u770B\u5230\u4EC0\u9EBC\u8B8A\u5316\u5462\uFF1F"
  - chapter: T2_3_6
    content: "\u5229\u7528\u78C5\u79E4\u79E4\u91CF\u53CD\u61C9\u5F8C\u7684\u5BF6\u7279\u74F6\u7E3D\u8CEA\u91CF\u3002"
  - chapter: T2_3_7
    content: "\u5404\u4F4D\u540C\u5B78\uFF0C\u63A5\u4E0B\u4F86\u8B93\u6211\u5011\u6253\u958B\u5BF6\u7279\u74F6\u3002"
  - chapter: T2_3_8
    content: "\u6C23\u7403\u5167\u6C23\u9AD4\u9038\u6563\u7121\u6CD5\u6E2C\u91CF\uFF0C\u6700\u5F8C\u518D\u5229\u7528\u78C5\u79E4\u6E2C\u91CF\u958B\u74F6\u5F8C\u7684\u5BF6\u7279\u74F6\u8207\u6C23\u7403\u7684\u7E3D\u8CEA\u91CF\u3002"
  - chapter: T2_3_9
    content: "\u5BE6\u9A57\u7D50\u679C\u53EF\u77E5\uFF0C\u5316\u5B78\u53CD\u61C9\u7684\u524D\u5F8C\u8CEA\u91CF\u662F\u5B88\u6046\u7684\u3002"
  - chapter: E2_1_1
    content: "\u8ACB\u5C07\u5C0F\u8607\u6253\u7C89\u5012\u5165\u6C34\u4E2D\uFF0C\u4E26\u4EE5\u73BB\u68D2\u652A\u62CC\u52A0\u5FEB\u6EB6\u89E3\uFF01"
  - chapter: E2_1_2
    content: "\u8ACB\u5229\u7528\u5C0F\u8607\u6253\u7C89\u6EB6\u6DB2\u8207\u62B9\u5E03\u5C07\u684C\u4E0A\u7684\u6CB9\u6C59\u64E6\u62ED\u4E7E\u6DE8\u5594\uFF01"
  - chapter: E2_2_1
    content: "\u5C0F\u8607\u6253\u52A0\u6AB8\u6AAC\u9178\uFF0C\u8D85\u7D1A\u8D77\u6CE1\u529B\uFF0C\u8D85\u5F37\u53BB\u6C59\u529B\u3002"
  - chapter: E2_2_2
    content: "\u5ABD\u5ABD\u4E4B\u524D\u6709\u8CB7\u4E00\u5305\uFF0C\u653E\u5728\u9019\u88E1\uFF0C\u6211\u5011\u4E5F\u4F86\u8A66\u8A66\u770B\u53BB\u6C59\u529B\u5427\uFF01"
  - chapter: E2_2_3
    content: "\u8ACB\u5C07\u5C0F\u8607\u6253\u8207\u6AB8\u6AAC\u9178\u7684\u6DF7\u5408\u7C89\u672B\u5012\u5165\u6C34\u4E2D\uFF01"
  - chapter: E2_3_1
    content: "\u5C07\u78B3\u7C89\u3001\u6C27\u5316\u9285\u7C89\u5012\u5165\u8A66\u7BA1\u4E2D\uFF0C\u79E4\u91CD\u3002"
  - chapter: E2_3_2
    content: "\u9EDE\u71C3\u9152\u7CBE\u71C8\uFF0C\u518D\u4EE5\u8A66\u7BA1\u593E\u593E\u53D6\u8A66\u7BA1\uFF0C\u653E\u5230\u706B\u7130\u4E0A\u52A0\u71B1\u3002"
  - chapter: E2_3_3
    content: "\u50BE\u659C\u8A66\u7BA1\u4E26\u4F86\u56DE\u5728\u706B\u7130\u4E0A\u52A0\u71B1\u3002"
  - chapter: E2_3_4
    content: "\u8A66\u7BA1\u52A0\u71B1\u6642\u9700\u8981\u50BE\u659C\uFF0C\u4E26\u4E14\u5F80\u8FD4\u79FB\u52D5\uFF0C\u5426\u5247\u8A66\u7BA1\u5BB9\u6613\u7834\u88C2\u5594\uFF01"
  - chapter: E2_3_5
    content: "\u8ACB\u5C07\u8A66\u7BA1\u653E\u7F6E\u65BC\u67B6\u4E0A\uFF0C\u79E4\u53CD\u61C9\u5F8C\u8CEA\u91CF\u3002"
